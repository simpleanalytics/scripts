  "bugs": {
    "url": "https://github.com/simpleanalytics/scripts/issues"
  },
  "homepage": "https://github.com/simpleanalytics/scripts#readme",
  "devDependencies": {
    "acorn": "^8.11.3",
    "browserstack-local": "^1.5.1",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "esprima": "^4.0.1",
    "handlebars": "^4.7.6",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "prettier": "3.2.5",
    "request": "^2.88.0",
    "selenium-webdriver": "^4.3.1",
    "uglify-js": "^3.9.4",

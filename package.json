    "posttest": "npm run build",
    "prettier": "prettier --check .",
    "test:unit": "mocha ./test/unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpleanalytics/scripts.git"
  },
  "keywords": [],
